{
	"$schema": "https://shadcn-svelte.com/schema/registry-item.json",
	"name": "community1",
	"title": "Community1",
	"type": "registry:component",
	"description": "A component featuring a logo, a call-to-action for joining a community, and social buttons.",
	"dependencies": [
		"@iconify/svelte"
	],
	"devDependencies": [
		"@iconify/svelte@^5.0.2"
	],
	"registryDependencies": [
		"button"
	],
	"meta": {
		"author": "mahyarmirrashed"
	},
	"files": [
		{
			"content": "<script lang=\"ts\" module>\n  import { Button } from \"$UI$/button/index.js\";\n  import { cn } from \"$UTILS$.js\";\n  import Icon from \"@iconify/svelte\";\n  import type { HTMLAttributes } from \"svelte/elements\";\n\n  type Social = {\n    url: string;\n    icon: string;\n  };\n\n  type Community1Props = {\n    logo?: {\n      src: string;\n      alt: string;\n      title: string;\n      subtitle: string;\n    };\n    socials?: Social[];\n  };\n</script>\n\n<script lang=\"ts\">\n  let {\n    logo = {\n      src: \"https://deifkwefumgah.cloudfront.net/shadcnblocks/block/logos/shadcnblockscom-icon.svg\",\n      alt: \"logo\",\n      title: \"Join our community\",\n      subtitle: \"of designers & developers\",\n    },\n    socials = [\n      { url: \"https://x.com/shadcnblocks\", icon: \"simple-icons:twitter\" },\n      { url: \"https://github.com/shadcnblocks\", icon: \"simple-icons:github\" },\n      { url: \"https://shadcnblocks.com\", icon: \"simple-icons:discord\" },\n    ],\n    class: className,\n    ...restProps\n  }: Community1Props & HTMLAttributes<HTMLElement> = $props();\n</script>\n\n<section class={cn(\"py-32\", className)} {...restProps}>\n  <div class=\"container\">\n    <div class=\"flex flex-col items-center gap-5\">\n      <img src={logo.src} alt={logo.alt} class=\"size-10\" />\n      <h2 class=\"text-center text-3xl font-semibold\">\n        {logo.title}\n        <br />\n        <span class=\"text-muted-foreground/80\">\n          {logo.subtitle}\n        </span>\n      </h2>\n      <div class=\"flex items-center gap-4\">\n        {#each socials as social}\n          <Button size=\"icon\" variant=\"outline\" asChild>\n            <a\n              href={social.url}\n              target=\"_blank\"\n              class=\"flex size-10 items-center justify-center\"\n            >\n              <Icon icon={social.icon} class=\"size-4\" />\n            </a>\n          </Button>\n        {/each}\n      </div>\n    </div>\n  </div>\n</section>\n",
			"type": "registry:component",
			"target": "community1.svelte"
		}
	]
}